{
	"name": "FlexiSkin",
	"version": "3.2.0-alpha",
	"license-name": "GPL-3.0-only",
	"url": "https://en.wiki.bluespice.com/wiki/Reference:FlexiSkin",
	"author": [
		"Robert Vogel",
		"Daniel Vogel"
	],
    "descriptionmsg": "flexiskin-extension-desc",
    "namemsg": "flexiskin-extension-name",
	"type": "specialpage",
	"attributes": {
		"BlueSpiceFoundation": {
			"PermissionRegistry": {
				"flexiskin-viewspecialpage": {
					"type": "global",
					"roles": [ "admin" ]
				},
				"flexiskin-api": {
					"type": "global",
					"roles": [ "admin" ]
				}
            },
            "RendererRegistry": {
            }
		},
		"FlexiSkin": {
			"PluginRegistry": {
				"WikiBackground": "MediaWiki\\Extension\\FlexiSkin\\Plugin\\WikiBackground::factory",
				"LogoAndFavicon": "MediaWiki\\Extension\\FlexiSkin\\Plugin\\LogoAndFavicon::factory",
				"Colors": "MediaWiki\\Extension\\FlexiSkin\\Plugin\\Colors::factory",
				"FreeCSS": "MediaWiki\\Extension\\FlexiSkin\\Plugin\\FreeCSS::factory"
			}
		}
    },
    "MessagesDirs": {
		"FlexiSkin": [
			"i18n"
		]
	},
	"AvailableRights": [
		"flexiskin-viewspecialpage",
		"flexiskin-api"
	],
	"ServiceWiringFiles": [
		"ServiceWiring.php"
	],
    "SpecialPages": {
		"FlexiSkin": "MediaWiki\\Extension\\FlexiSkin\\Special\\FlexiSkin"
	},
	"ExtensionMessagesFiles": {
		"FlexiSkinAlias": "languages/FlexiSkin.alias.php"
	},
    "AutoloadNamespaces": {
        "MediaWiki\\Extension\\FlexiSkin\\": "src/",
		"MediaWiki\\Extension\\FlexiSkin\\Tests\\": "tests/phpunit/"
    },
	"APIModules": {
		"flexiskin-activate": "MediaWiki\\Extension\\FlexiSkin\\Api\\ActivateFlexiSkin",
		"flexiskin-save": "MediaWiki\\Extension\\FlexiSkin\\Api\\SaveFlexiSkin",
		"flexiskin-preview": "MediaWiki\\Extension\\FlexiSkin\\Api\\PreviewFlexiSkin",
		"flexiskin-new": "MediaWiki\\Extension\\FlexiSkin\\Api\\NewFlexiSkin",
		"flexiskin-delete": "MediaWiki\\Extension\\FlexiSkin\\Api\\DeleteFlexiSkin",
		"flexiskin-restore": "MediaWiki\\Extension\\FlexiSkin\\Api\\RestoreFlexiSkin"
	},
	"Hooks": {
		"BeforePageDisplay": [
			"\\MediaWiki\\Extension\\FlexiSkin\\Hook\\BeforePageDisplay\\GetActiveSkins::callback",
			"\\MediaWiki\\Extension\\FlexiSkin\\Hook\\BeforePageDisplay\\AddActiveSkinStyles::callback"
		],
		"LoadExtensionSchemaUpdates": "\\MediaWiki\\Extension\\FlexiSkin\\Hook\\LoadExtensionSchemaUpdates\\FlexiSkinShemaUpdates::callback",
		"OutputPageBodyAttributes": "\\MediaWiki\\Extension\\FlexiSkin\\Hook\\OutputPageBodyAttributes\\AddFlexiSkinBodyClass::callback"
    },
    "ResourceModules": {
		"ext.flexiskin.styles": {
			"class": "\\MediaWiki\\Extension\\FlexiSkin\\ResourceLoader\\ActiveFlexiSkin"
		},
		"ext.flexiskin.specialpage.styles": {
			"styles": [
				"stylesheets/selector.less"
			]
		},
		"ext.flexiskin.specialpage.scripts": {
			"class": "\\MediaWiki\\Extension\\FlexiSkin\\ResourceLoader\\Configurator",
			"scripts": [
				"js/ui/dialog/NewSkin.js",
				"js/ui/Selector.js",
				"js/ui/BookletPage.js",
				"js/ui/Configurator.js",
				"js/ui/Manager.js",
				"js/ui/flexiskin.js"
			],
			"dependencies" : [
				"ext.bluespice",
				"mediawiki.api",
				"oojs",
				"oojs-ui",
				"ext.codeEditor.ace",
				"ext.oOJSPlus.widgets.colorPicker"
			],
			"messages": [
				"flexiskin-ui-no-skins",
				"flexisin-ui-active-skin",
				"flexiskin-ui-no-active-skin"
			]
		}
    },
	"config": {
		"FlexiSkinThemePath": {
			"value": "resources/templates/themes",
			"path": true
		}
    },
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "FlexiSkin/resources"
	},
	"manifest_version": 2,
	"load_composer_autoloader": true
}
